[sub_resource type="GDScript" id="GDScript_lfrn8"]
resource_name = "player_cards"
script/source = "extends Node2D

func update_game() -> void:
	print(\"update_game called\")
	for x in range(len(globals.player_cards)) :
		if x < globals.player_dealt_amount :
			get_node(\"Card\"+str(x+1)).set(\"visible\", true)
			get_node(\"Card\"+str(x+1)+\"/Value\").set(\"text\", globals.player_cards[x])
		else :
			break
	print(\"updated!\")
"

[sub_resource type="GDScript" id="GDScript_5i0tg"]
resource_name = "opponent_cards"
