[sub_resource type="GDScript" id="GDScript_bdhmt"]
resource_name = "dev_menu"
script/source = "extends Control

signal update

var g = globals

func update_dev_menu() -> void:
	# Cards
	get_node(\"Menus/CardValues/Card1\").set(\"value\", g.player_cards[0])
	get_node(\"Menus/CardValues/Card2\").set(\"value\", g.player_cards[1])
	get_node(\"Menus/CardValues/Card3\").set(\"value\", g.player_cards[2])
	get_node(\"Menus/CardValues/Card4\").set(\"value\", g.player_cards[3])
	get_node(\"Menus/CardValues/Card5\").set(\"value\", g.player_cards[4])
	# Values
	get_node(\"Menus/Other/Label\").set(\"value\", g.player_cards[0])

func _on_dev_mode_enabled() -> void:
	update_dev_menu()
	visible = true

func _on_resume_pressed() -> void:
	visible = false

func _on_card_1_value_changed(value) -> void:
	g.player_cards[0] = int(value)

func _on_card_2_value_changed(value) -> void:
	g.player_cards[1] = int(value)

func _on_card_3_value_changed(value) -> void:
	g.player_cards[2] = int(value)

func _on_card_4_value_changed(value) -> void:
	g.player_cards[3] = int(value)

func _on_card_5_value_changed(value) -> void:
	g.player_cards[4] = int(value)
"

